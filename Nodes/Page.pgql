# pho-graphql-v1

type Page implements ActorNode 
@edges(in:"User:Follow, StatusUpdate:Mention, User:Consume, User:Message", out:"Post, React, Comment, Follow, Consume, Message")
@permissions(mod: "0x0f754", mask: "0xfffff") 
@properties(editable: true, volatile: false, revisionable: false)
{
    id: ID!,
    password: String! @md5 @constraints(regex: "^[a-zA-Z0-9_]{4,12}$"),
    join_time: Date! @now
    about: String @constraints(maxLength: 255) @default(String: "")
}
